#include "holberton.h"
#include <stdio.h>

/**
 * main - check the code for Holberton School students.
 * @argc: argument counter
 * @argv: argument v.
 * Return: Always 0.
 */
int main(int argc, char *argv[])
{
	int resultado;

	if (argc == 3)
	{
		resultado = *argv[1] * *argv[2];
		printf("%d\n", resultado);
		return (0);
	}
	printf("Error");
	return (1);
}
